<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_Cost_Positive</fullName>
    <active>true</active>
    <description>Unit cost must be positive</description>
    <errorConditionFormula>Unit_Cost__c &lt;= 0</errorConditionFormula>
    <errorDisplayField>Unit_Cost__c</errorDisplayField>
    <errorMessage>Unit cost must be greater than zero</errorMessage>
</ValidationRule>
